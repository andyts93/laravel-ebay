{
    "name": "andyts93/laravel-ebay",
    "description": "Laravel package to integrate eBay REST API",
    "keywords": ["laravel", "ebay", "ecommerce", "api", "marketplace"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Castellini",
            "email": "and1@live.it"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Andyts93\\LaravelEbay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andyts93\\LaravelEbay\\EbayServiceProvider"
            ],
            "aliases": [
                "Andyts93\\LaravelEbay\\Facades\\EbayApi"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
